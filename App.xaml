<Application x:Class="WesternLauncherOfEasternOrigins.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WesternLauncherOfEasternOrigins"
             StartupUri="Main/TheLauncher.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                
                <ResourceDictionary Source="Styles/Colors.xaml"/>

                <ResourceDictionary Source="Styles/TabControlStyle.xaml"/>
                <ResourceDictionary Source="Styles/LabelStyle.xaml"/>
                <ResourceDictionary Source="Styles/ButtonStyle.xaml"/>

                <ResourceDictionary Source="Styles/ContextMenuStyle.xaml"/>
                <ResourceDictionary Source="Styles/MenuStyle.xaml"/>


                
            </ResourceDictionary.MergedDictionaries>

            <DrawingImage x:Key="CheckIcon">
                <DrawingImage.Drawing>
                    <GeometryDrawing Brush="White" Geometry="M 0 0 L 10 10 L 20 0 Z" />
                </DrawingImage.Drawing>
            </DrawingImage>

        </ResourceDictionary>
    </Application.Resources>
</Application>
