<Application x:Class="WesternLauncherOfEasternOrigins.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WesternLauncherOfEasternOrigins"
             StartupUri="Main/TheLauncher.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/PixelWPF;component/PixelDictionary.xaml" />

                <ResourceDictionary Source="Styles/TouhouColors.xaml"/>
                <ResourceDictionary Source="Styles/TouhouStyle.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>           

        </ResourceDictionary>
    </Application.Resources>
</Application>
